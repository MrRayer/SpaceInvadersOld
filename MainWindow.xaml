<Window x:Class="SpaceInvaders.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SpaceInvaders"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="900"
        KeyDown="KeyIsDown"
        KeyUp="KeyIsUp">
    <Canvas Name="myCanvas" Background="Black" Focusable="False">
        <Rectangle Canvas.Left="400" Canvas.Top="370"  Name = "Player" Height = "40" Width = "80"/>
        <Rectangle Canvas.Left="0" Canvas.Top="360" Height = "1" Width = "900" Fill="White"/>
    </Canvas>
</Window>